<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>CalromSpecFlowRetryCore</id>
    <version>1.2.0</version>
    <title>CalromSpecFlowRetryCore</title>
    <authors>Julio C. Tejero</authors>
    <owners>Calrom Ltd.</owners>
    <description>Retry for SpecFlow</description>
    <summary>Plugin for Retrying failing Nunit tests generated by Specflow</summary>
    <language>en-UK</language>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>specflow</tags>
    <dependencies>
      <dependency id="SpecFlow" version="3.4.8"/>
    </dependencies>
  </metadata>
  <files>
    <file src="build\**\*" target="build"/>
    <file src="bin\$config$\net471\SampleGeneratorPlugin.SpecFlowPlugin.*" target="build\net471"/>l
    <file src="bin\$config$\netcoreapp3.1\SampleGeneratorPlugin.SpecFlowPlugin.dll" target="build\netcoreapp2.1"/>
    <file src="bin\$config$\netcoreapp3.1\SampleGeneratorPlugin.SpecFlowPlugin.pdb" target="build\netcoreapp2.1"/>
  </files>
</package>